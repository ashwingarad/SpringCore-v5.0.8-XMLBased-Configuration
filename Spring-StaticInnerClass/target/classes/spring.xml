<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="employee" class="com.model.Employee">
		<property name="id" value="1"></property>
		<property name="name" value="Ashwin"></property>
		<property name="salary" value="254624"></property>
	</bean>

	<bean id="address" class="com.model.Employee$Address">
		<property name="street" value="Vimannagar"></property>
		<property name="pincode" value="411014"></property>
	</bean>

</beans>